    "eslint": "./node_modules/eslint/bin/eslint.js './**/*.js' './**/*.jsx'",
    "build:server": "babel server.js -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ah-med/FastFoodFast-React.git"
